{
  "presets": [
    "next/babel"
  ],
  "env": {
    "development": {
      "plugins": [
        [
          "styled-components", 
          {
            "ssr": true, 
            "displayName": true, 
            "preprocess": false 
          }
        ],
        [
          "inline-dotenv",{
            "path": "./env"
          }
        ]
      ]
    },
    "production": {
      "plugins": [
        [
          "styled-components", 
          {
            "ssr": true, 
            "displayName": true, 
            "preprocess": false 
          }
        ],
        [
          "inline-dotenv",{
            "path": "./env.production"
          }
        ]
      ]
    }
  }
}